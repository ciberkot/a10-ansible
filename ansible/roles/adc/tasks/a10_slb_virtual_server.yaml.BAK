---
- name: Create a10_slb_virtual_server instance
  a10_slb_virtual_server:
      a10_host: "{{ inventory_hostname }}"
      a10_username: "{{ a10_username }}"
      a10_password: "{{ a10_password }}"
      a10_port: "{{ a10_port }}"
      a10_protocol: "{{ a10_protocol }}"
      name: vip4
      ip_address: 4.4.4.1
      port_list: 
      - {'protocol': 'https', 'port-number': 443, "service-group":"ANSIBLE-SG1"}
      - {'protocol': 'http', 'port-number': 80, "service-group":"ANSIBLE-SG1"}
      - {'protocol': 'tcp', 'port-number': 32, "service-group":"ANSIBLE-SG1"}
